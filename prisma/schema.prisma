// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  user_id  String    @id @unique @default(uuid())
  nickname String    @unique
  email    String    @unique
  password String
  projects Project[]

  @@map("user")
}

model Project {
  project_id  String   @id @unique @default(uuid())
  imageLink   String
  name        String
  type        String
  description String[]
  date        String
  links       String[]
  engines     String[]
  User        User?    @relation(fields: [userUser_id], references: [user_id])
  userUser_id String?

  @@map("projects")
}
